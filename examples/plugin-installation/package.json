{
  "name": "jest-plugins-example-plugin-installation",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "jest": "^20.0.4",
    "jest-plugin-action": "*",
    "jest-plugin-console-matchers": "*",
    "jest-plugin-context": "*",
    "jest-plugin-its": "*",
    "jest-plugin-set": "*",
    "jest-plugin-unhandled-promise": "*",
    "jest-plugins": "*"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-plugins/setup",
    "testEnvironment": "node"
  },
  "jest-plugins": [
    "jest-plugin-action",
    "jest-plugin-console-matchers",
    "jest-plugin-context",
    "jest-plugin-its",
    "jest-plugin-set",
    "jest-plugin-unhandled-promise"
  ]
}
